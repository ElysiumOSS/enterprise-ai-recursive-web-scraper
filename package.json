{
	"name": "enterprise-ai-recursive-web-scraper",
	"version": "1.1.2",
	"description": "AI powered, recursive, web-scraper utilizing Gemini models, Puppeteer, and Playwright",
	"keywords": [
		"web-scraper",
		"ai",
		"gemini",
		"puppeteer",
		"playwright",
		"typescript",
		"cli"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ElysiumOSS/enterprise-ai-recursive-web-scraper.git"
	},
	"license": "MIT",
	"author": {
		"name": "WomB0ComB0",
		"email": "airwolf635@gmail.com"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./lib/index.js",
			"types": "./lib/index.d.ts"
		},
		"./cli": {
			"import": "./lib/cli.js",
			"types": "./lib/cli.d.ts"
		}
	},
	"main": "./lib/cli.js",
	"bin": {
		"enterprise-ai-recursive-web-scraper": "lib/cli-wrapper.js"
	},
	"files": [
		"lib/*.js",
		"lib/*.d.ts",
		"lib/*.map",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "bun run build:pre && bun run build:tsc",
		"build:pre": "bunx puppeteer browsers install chrome",
		"build:tsc": "tsc && echo '#!/usr/bin/env node' > lib/cli-wrapper.js && echo 'import(\"./cli.js\");' >> lib/cli-wrapper.js && chmod +x lib/cli-wrapper.js",
		"prepack": "bun run build",
		"start": "bun run cli",
		"cli": "bun lib/cli.js",
		"prepublishOnly": "bun run build",
		"lint": "bunx biome check --write ./ || true",
		"pretest": "bun run build",
		"test": "bun test"
	},
	"dependencies": {
		"@biomejs/biome": "2.2.5",
		"@google/genai": "^1.22.0",
		"@types/bun": "^1.2.23",
		"async-sema": "^3.1.1",
		"bottleneck": "^2.19.5",
		"chalk": "^5.6.2",
		"cli-table3": "^0.6.5",
		"commander": "^14.0.1",
		"dotenv": "^17.2.3",
		"enterprise-ai-recursive-web-scraper": "^1.1.2",
		"inquirer": "^12.9.6",
		"lru-cache": "^11.2.2",
		"natural": "^8.1.0",
		"ora": "^9.0.0",
		"playwright": "^1.55.1",
		"pretty-bytes": "^7.1.0",
		"puppeteer": "^24.23.0",
		"puppeteer-extra": "^3.3.6",
		"puppeteer-extra-plugin-adblocker": "^2.13.6",
		"puppeteer-extra-plugin-stealth": "^2.11.2",
		"vitest": "^3.2.4",
		"winston": "^3.18.3"
	},
	"devDependencies": {
		"@types/inquirer": "^9.0.9",
		"@types/node": "^24.6.2",
		"tsup": "^8.5.0",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=20.12.2"
	},
	"packageManager": "bun@1.2.23"
}
