{
	"name": "enterprise-ai-recursive-web-scraper",
	"version": "0.0.0",
	"description": "Ai powered, recursive, web-scraper. Utilizing gemini model's, puppeteer, and playwright",
	"repository": {
		"type": "git",
		"url": "https://github.com/WomB0ComB0/enterprise-ai-recursive-web-scraper"
	},
	"license": "MIT",
	"author": {
		"name": "WomB0ComB0",
		"email": "airwolf635@gmail.com"
	},
	"type": "module",
	"main": "./lib/index.js",
	"files": [
		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "bun run build:pre && tsup",
		"build:pre": "bunx puppeteer browsers install chrome",
		"format": "prettier .",
		"lint": "eslint . --max-warnings 0",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"prepare": "husky",
		"test": "vitest",
		"tsc": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"dependencies": {
		"@google/generative-ai": "^0.21.0",
		"dotenv": "^16.4.5",
		"husky": "^9.1.6",
		"playwright": "^1.48.2",
		"prettier": "^3.3.3",
		"prettier-plugin-curly": "^0.3.1",
		"prettier-plugin-packagejson": "^2.5.3",
		"prettier-plugin-sh": "^0.14.0",
		"puppeteer": "^latest",
		"puppeteer-extra": "^latest",
		"puppeteer-extra-plugin-adblocker": "^latest",
		"puppeteer-extra-plugin-stealth": "^latest"
	},
	"devDependencies": {},
	"packageManager": "bun@1.1.34",
	"engines": {
		"node": ">=20.12.2"
	},
	"publishConfig": {
		"provenance": true
	}
}
